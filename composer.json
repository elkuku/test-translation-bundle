{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "beta",
    "prefer-stable": true,
    "require": {
        "php": ">=8.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/doctrine-bundle": "^2.10",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^3",
        "elkuku/symfony-utils": "dev-master",
        "google/apiclient": "^2.15",
        "knpuniversity/oauth2-client-bundle": "^v2.17.0",
        "league/commonmark": "^2.7",
        "league/oauth2-github": "^3.1",
        "league/oauth2-google": "^4.0",
        "php-cs-fixer/shim": "^3.88",
        "phpdocumentor/reflection-docblock": "^5.3",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "*",
        "symfony/asset-mapper": "*",
        "symfony/console": "*",
        "symfony/doctrine-messenger": "*",
        "symfony/dotenv": "*",
        "symfony/expression-language": "*",
        "symfony/flex": "^2",
        "symfony/form": "*",
        "symfony/framework-bundle": "*",
        "symfony/http-client": "*",
        "symfony/intl": "*",
        "symfony/mailer": "*",
        "symfony/mime": "*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "*",
        "symfony/process": "*",
        "symfony/property-access": "*",
        "symfony/property-info": "*",
        "symfony/runtime": "*",
        "symfony/security-bundle": "*",
        "symfony/serializer": "*",
        "symfony/stimulus-bundle": "^2.13",
        "symfony/string": "*",
        "symfony/translation": "*",
        "symfony/twig-bundle": "*",
        "symfony/ux-icons": "^2.18",
        "symfony/ux-twig-component": "^2.18",
        "symfony/validator": "*",
        "symfony/web-link": "*",
        "symfony/yaml": "*",
        "symfonycasts/object-translation-bundle": "@dev",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/markdown-extra": "^3.21",
        "twig/string-extra": "^3.21",
        "twig/twig": "^2.12|^3.0"
    },
    "repositories": [
        {
            "type": "path",
            "url": "object-translation-bundle"
        }
    ],
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "check" : [
            "echo 'Check for outdated assets:'",
            "bin/console importmap:outdated",
            "echo 'Check for outdated PHP deps:'",
            "composer outdated",
            "echo 'Check for outdated Recipes:'",
            "composer recipes:update"
            ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "7.3.*"
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^4.0",
        "friendsofphp/php-cs-fixer": "^3.86",
        "jawira/doctrine-diagram-bundle": "dev-main",
        "phpstan/phpdoc-parser": "^2",
        "phpstan/phpstan": "^2",
        "phpunit/phpunit": "^12",
        "rector/rector": "^2",
        "roave/security-advisories": "dev-latest",
        "symfony/browser-kit": "*",
        "symfony/css-selector": "*",
        "symfony/debug-bundle": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^7.0",
        "symfony/stopwatch": "*",
        "symfony/web-profiler-bundle": "*",
        "zenstruck/foundry": "^2.7"
    }
}
